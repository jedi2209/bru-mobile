{
  "name": "bruapp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "yarn clean:android; react-native run-android --active-arch-only",
    "android-build": "yarn clean:android; bundle exec fastlane android internal",
    "build": "yarn android-build; yarn ios-build;",
    "clean": "react-native-clean-project",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:ios": "xattr -w com.apple.xcode.CreatedByBuildSystem true $(pwd)/ios/build; cd ios && xcodebuild clean && cd ../",
    "ios": "yarn clean:ios; yarn run iphoneProMax",
    "ios-build": "bundle exec fastlane ios beta; rm bruapp.app.dSYM.zip bruapp.ipa;",
    "iphone": "yarn clean:ios; react-native run-ios --simulator=\"iPhone 14\"",
    "iphonePro": "yarn clean:ios; react-native run-ios --simulator=\"iPhone 14 Pro\"",
    "iphoneProMax": "yarn clean:ios; react-native run-ios --simulator=\"iPhone 14 Pro Max\"",
    "lint": "eslint .",
    "postinstall": "npx patch-package && cd ios && pod update && cd ../ && npx pod-install;",
    "react": "watchman watch-del $(pwd);  watchman watch-project $(pwd); yarn start;",
    "start": "react-native start --resetCache",
    "test": "jest",
    "version": "./version.sh"
  },
  "dependencies": {
    "@dank-style/animation-plugin": "latest",
    "@dank-style/react": "latest",
    "@gluestack-ui/hstack": "latest",
    "@gluestack-ui/provider": "latest",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-firebase/analytics": "^17.5.0",
    "@react-native-firebase/app": "^17.5.0",
    "@react-native-firebase/app-check": "^17.5.0",
    "@react-native-firebase/perf": "^17.5.0",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "effector": "^22.8.5",
    "effector-react": "^22.5.1",
    "react": "18.2.0",
    "react-native": "0.72.0-rc.3",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-localization": "^2.3.1",
    "react-native-paper": "^5.8.0",
    "react-native-reanimated": "^3.1.0",
    "react-native-safe-area-context": "^4.5.2",
    "react-native-screens": "^3.20.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.9.0",
    "react-native-vector-icons": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/eslint-config": "^0.72.1",
    "@react-native/metro-config": "^0.72.5",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^29.2.1",
    "@types/metro-config": "^0.76.3",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.71.7",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "effector-logger": "^0.14.0",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.4",
    "patch-package": "^7.0.0",
    "prettier": "^2.4.1",
    "react-native-clean-project": "^4.0.1",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-web": "^0.18.12",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
